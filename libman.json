{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@5.3.3",
      "destination": "wwwroot/lib/bootstrap/dist/",
      "files": [
        "css/bootstrap.min.css",
        "css/bootstrap.min.css.map",
        "js/bootstrap.bundle.min.js",
        "js/bootstrap.bundle.min.js.map"
      ]
    },
    {
      "library": "jquery@3.7.1",
      "destination": "wwwroot/lib/jquery/dist/",
      "files": [
        "jquery.min.js",
        "jquery.min.map"
      ]
    },
    {
      "library": "jquery-validate@1.21.0",
      "destination": "wwwroot/lib/jquery-validation/dist/",
      "files": [
        "jquery.validate.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@4.0.0",
      "provider": "unpkg",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "dist/jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "library": "microsoft-signalr@7.0.0",
      "destination": "wwwroot/lib/microsoft-signalr/",
      "files": [
        "signalr.js",
        "signalr.min.js"
      ]
    }
  ]
}
